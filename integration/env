# Integration config example

# Bluemix API endpoint
export BX_API="api.foo.bar"
export BX_API_KEY="def"
# k8s cluster name
export CLUSTER_NAME="my-cluster"
# Softlayer account username, typically starts with the SL account number
export SL_USERNAME="123_me@me.com"
# Softlayer API key
export SL_API_KEY="456"
