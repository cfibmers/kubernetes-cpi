# Integration config example

# bluemix env API endpoint
export BX_API="api.foo.bar"
# bluemix user name
export BX_USERNAME="blah@blah.com"
# BX_API_KEY is only required if your ID is federated
export BX_API_KEY="def"
# BX_PASSWORD can be empty if using BX_API_KEY
export BX_PASSWORD="abc"
# bluemix user's *BSS* acct
export BX_ACCOUNTID="123"
# k8s cluster name
export CLUSTER_NAME="my-cluster"
# Softlayer acct user name, typically starts with the SL acct number
export SL_USERNAME="123_me@me.com"
# Softlayer apikey token
export SL_API_KEY="456"
